{
    "name": "doz-formfield",
    "version": "0.1.9",
    "description": "All form elements component for DOZ",
    "main": "dist/bundle.js",
    "scripts": {
        "start": "webpack-dev-server --open",
        "build": "webpack --progress --mode=production",
        "test": "mocha test",
        "release:before": "npm run-script test",
        "release:major": "webpack --env.major --mode=production && npm run-script release:publish",
        "release:minor": "webpack --env.minor --mode=production && npm run-script release:publish",
        "release:patch": "webpack --env.patch --mode=production && npm run-script release:publish",
        "release:publish": "git commit -a -m BUILD && version-to-tag.sh && npm publish"
    },
    "keywords": [
        "doz",
        "component"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dozjs-cmp/doz-formfield.git"
    },
    "bugs": {
        "url": "https://github.com/dozjs-cmp/doz-formfield/issues"
    },
    "homepage": "https://github.com/dozjs-cmp/doz-formfield#readme",
    "author": "Pietro Lembo",
    "license": "MIT",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.6.1",
        "babel-register": "^6.26.0",
        "babel-runtime": "^6.26.0",
        "chai": "^4.2.0",
        "css-loader": "^0.28.11",
        "doz": "^1.15.0",
        "hard-source-webpack-plugin": "^0.12.0",
        "ignore-styles": "^5.0.1",
        "jsdoc-to-markdown": "^4.0.1",
        "jsdom": "^11.12.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^5.0.4",
        "style-loader": "^0.21.0",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "unminified-webpack-plugin": "^2.0.0",
        "webpack": "^4.26.0",
        "webpack-auto-inject-version": "^1.2.2",
        "webpack-cli": "^3.1.2",
        "webpack-dev-server": "^3.2.1"
    },
    "dependencies": {
        "axios": "^0.18.0",
        "typis": "^1.1.1"
    }
}